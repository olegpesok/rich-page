{
  "name": "Rich Page",
  "manifest_version": 2,
  "version": "2.3.0",
  "description": "(Very temporary name) inc.",
  "permissions": [
    "https://localhost:8888/*",
    "http://rich-page.appspot.com/*"
  ],
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["bower_components/jquery/jquery.js", "contentscript.js"]
    }
  ]
}

