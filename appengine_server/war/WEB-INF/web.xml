<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	
	<servlet>
		<servlet-name>SnippetServlet</servlet-name>
		<servlet-class>com.owow.rich.servlet.SnippetServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PageProccessServlet</servlet-name>
		<servlet-class>com.owow.rich.servlet.PageProccessServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SnippetServlet</servlet-name>
		<url-pattern>/Snippet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PageProccessServlet</servlet-name>
		<url-pattern>/PageProccessServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
